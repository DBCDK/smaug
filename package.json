{
  "name": "openserviceprovider",
  "version": "0.0.1",
  "description": "ServiceProvider Application",
  "main": "src/main.js",
  "scripts": {
    "dev": "npm run serve -s",
    "lint": "eslint ./src",
    "lint:checkstyle": "eslint ./src  -o report.xml -f checkstyle",
    "serve": "supervisor  --instant-kill -w src/app.js,src/server,node_modules src/main.js",
    "start": "node src/main.js",
    "test": "karma start --single-run=true",
    "test:watch": "karma start",
    "tail": "tail -f log.log",
    "tail:timer": "tail -f log.log | awk '(/event/ || /time_delta/ || /TIMER/) && !/result/'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DBCDK/openserviceprovider.git"
  },
  "keywords": [
    "pallesgavebod",
    "dbc"
  ],
  "author": "dbcdk",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DBCDK/openserviceprovider/issues"
  },
  "homepage": "https://github.com/DBCDK/openserviceprovider#readme",
  "dependencies": {
    "@dbcdk/dbc-config": "^2.0.0",
    "babel": "5.8.29",
    "babel-core": "5.8.29",
    "body-parser": "^1.14.1",
    "dbc-node-borchk": "^2.0.1",
    "express": "4.13.3",
    "lodash": "^3.10.1",
    "oauth2-server": "^2.4.1",
    "socket.io": "1.3.7"
  },
  "devDependencies": {
    "babel-eslint": "^4.0.5",
    "eslint": "^1.0.0"
  }
}
